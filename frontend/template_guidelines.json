        "backgroundColor": "#FFFFFF",
        "color": "#6B7280",
        "padding": "8px 12px",
        "border": "1px solid #E5E7EB",
        "borderRadius": "6px",
        "fontSize": "14px",
        "fontWeight": 500,
        "cursor": "pointer",
        "transition": "all 200ms ease",
        "hover": {
          "backgroundColor": "#F9FAFB",
          "color": "#111827"
        }
      },
      "icon": {
        "backgroundColor": "transparent",
        "color": "#6B7280",
