              "fontWeight": 500,
              "color": "#9CA3AF",
              "marginBottom": "2px"
            },
            "title": {
              "fontSize": "14px",
              "fontWeight": 500,
              "color": "#111827"
            }
          }
        },
        "right": {
          "textAlign": "right",
          "value": {
            "fontSize": "14px",
            "fontWeight": 600,
            "color": "#111827"
          }
        }
      }
    },
    "button": {
      "primary": {
        "backgroundColor": "#334E68",
        "color": "#FFFFFF",
        "padding": "10px 16px",
        "borderRadius": "8px",
        "border": "none",
        "fontSize": "14px",
        "fontWeight": 600,
        "cursor": "pointer",
        "display": "inline-flex",
        "alignItems": "center",
        "gap": "8px",
        "transition": "all 200ms ease",
        "hover": {
