          "padding": "12px 20px",
          "backgroundColor": "#FAFAFA",
          "display": "flex",
          "alignItems": "center",
          "justifyContent": "space-between",
          "cursor": "pointer",
          "title": {
            "fontSize": "14px",
            "fontWeight": 600,
            "color": "#6B7280"
          },
          "value": {
            "fontSize": "16px",
            "fontWeight": 700,
            "color": "#111827"
          }
