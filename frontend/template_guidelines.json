      "base": {
        "padding": "4px 8px",
        "borderRadius": "6px",
        "fontSize": "12px",
        "fontWeight": 600,
        "display": "inline-flex",
        "alignItems": "center",
        "justifyContent": "center",
        "textAlign": "center",
        "minWidth": "20px",
        "height": "20px"
      }
    },
    "chart": {
      "donut": {
        "size": "120px",
        "strokeWidth": "16px",
        "backgroundColor": "#F9FAFB"
