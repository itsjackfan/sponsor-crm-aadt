      "height": "100vh",
      "overflowY": "auto",
      "brand": {
        "padding": "0 24px 32px 24px",
        "display": "flex",
        "alignItems": "center",
        "logo": {
          "width": "32px",
          "height": "32px",
          "backgroundColor": "#334E68",
          "borderRadius": "8px",
          "display": "flex",
          "alignItems": "center",
          "justifyContent": "center",
          "color": "#FFFFFF",
          "fontSize": "16px",
          "fontWeight": 700
        },
        "text": {
          "fontSize": "20px",
          "fontWeight": 700,
          "color": "#111827",
          "marginLeft": "12px"
        }
      },
      "navItems": {
        "padding": "12px 24px",
        "fontSize": "16px",
        "fontWeight": 500,
        "color": "#6B7280",
        "textDecoration": "none",
        "display": "flex",
        "alignItems": "center",
        "gap": "12px",
        "borderRadius": "0px",
        "transition": "all 200ms ease",
        "hover": {
          "color": "#111827",
          "backgroundColor": "#F9FAFB"
        },
        "active": {
          "color": "#334E68",
          "backgroundColor": "#F0F4F8",
          "fontWeight": 600
        }
      },
      "icons": {
        "width": "20px",
        "height": "20px",
        "strokeWidth": "1.5px"
      }
    },
    "mainContent": {
      "marginLeft": "280px",
      "padding": "24px",
      "width": "calc(100% - 280px)",
      "maxWidth": "calc(1440px - 280px)"
    },
    "pageHeader": {
      "marginBottom": "32px",
      "title": {
        "fontSize": "24px",
        "fontWeight": 600,
        "color": "#111827",
        "lineHeight": "1.25"
      },
      "subtitle": {
        "fontSize": "14px",
        "fontWeight": 400,
        "color": "#6B7280",
        "marginTop": "4px"
      },
      "actions": {
        "display": "flex",
        "alignItems": "center",
        "gap": "12px",
        "marginTop": "16px"
      }
    },
    "contentGrid": {
      "display": "grid",
      "gridTemplateColumns": "320px 1fr",
      "gap": "24px",
      "marginBottom": "32px"
    },
