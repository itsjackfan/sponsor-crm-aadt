        "display": "flex",
        "alignItems": "flex-start",
        "justifyContent": "space-between",
        "marginBottom": "24px",
        "title": {
          "fontSize": "18px",
          "fontWeight": 600,
          "color": "#111827"
        },
        "subtitle": {
          "fontSize": "14px",
          "fontWeight": 400,
          "color": "#6B7280",
          "marginTop": "2px"
