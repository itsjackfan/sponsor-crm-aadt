      "details": {
        "display": "flex",
        "alignItems": "center",
        "justifyContent": "space-between",
        "left": {
          "fontSize": "12px",
          "fontWeight": 500,
          "color": "#6B7280"
        },
        "right": {
          "fontSize": "12px",
          "fontWeight": 500,
          "color": "#9CA3AF"
        }
      }
    },
